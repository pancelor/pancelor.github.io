{
  "name": "pancelor-dot-com",
  "version": "0.1.0",
  "description": "my personal website",
  "engines": {
    "node": "14.x"
  },
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all -p watch server",
    "watch": "nodemon build.js",
    "server": "http-server public -c-1 -p 8080",
    "build": "node build.js",
    "deploy": "git subtree push --prefix public origin gh-pages"
  },
  "nodemonConfig": {
    "watch": [
      "templates/"
    ],
    "ext": "ejs"
  },
  "dependencies": {
    "ejs": "^3.1.5"
  },
  "devDependencies": {
    "http-server": "^0.12.3",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5"
  }
}
